{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "indent": [true, "tabs", 4],
        "no-trailing-whitespace": false,
        "no-inferrable-types": false,
        "one-line": false,
        "max-line-length": [ false ],
        "member-ordering": [ false ]
    },
    "linterOptions": {
        "exclude": [
            "generated/**"
        ]
    }
}
